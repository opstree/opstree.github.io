<body>
<div class="container p-2">
    <nav class="navbar navbar-expand-lg bg-white">
        <div class="navbar-brand ps-5 p-2">
            <img src="./static/logo.png" alt="...">
        </div>
        <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent"
                aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
        </button>
        <div class="collapse navbar-collapse" id="navbarSupportedContent">
            <ul class="navbar-nav ms-auto">
                <li class="nav-item">
                    <a class="nav-link text-blue-custom fw-bold" href="https://www.opstree.com/" target="_blank">Website</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link text-blue-custom fw-bold" href="https://blog.opstree.com/" target="_blank">Blog</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link text-blue-custom fw-bold" href="/projects/" target="_blank">Projects</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="https://github.com/opstree"><img src="static/github.svg" class="img-fluid" width="32" height="32"></a>
                </li>
            </ul>
        </div>
    </nav>
</div>
<div class="container p-3">
    <div class="row ps-5">
    {% for data in opensource_projects %}
        <div class="col-sm-4">
            <div style="width: 30rem;" data-title={{ data.technology }}>
                <img class="card-img-top" src="./static/{{ data.logo }}" alt="{{ data.name }}" height="128" width="128">
                <div class="card-body">
                    <h2 class="card-title text-center mt-3 text-blue-custom">{{ data.name }}</h2>
                    <p class="card-text text-left font-10 p-1">{{ data.description }}</p>
                    <a href="{{ data.repo_link }}" target="_blank" class="p-2"><img src="static/github.svg" class="img-fluid" width="32" height="32"></a>
                    <a href="{{ data.documentation }}" target="_blank" class="p-2"><img src="static/documentation.svg" class="img-fluid" width="32" height="32"></a>
                    <a class="github-button p-2"
                       href="{{ data.repo_link }}"
                       data-icon="octicon-star"
                       data-size="large"
                       data-show-count="true"
                       aria-label="Star {{ data.repo_link }} on GitHub">Star</a>
                </div>
            </div>
        </div>
    {% endfor %}
    </div>
</div>
</body>
